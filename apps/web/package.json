{
  "name": "@vamsa/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "start": "bun run server/index.ts",
    "start:prod": "NODE_ENV=production bun run server/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "LOG_LEVEL=silent bun test --preload=./tests/setup/test-logger-mock.ts ./src ./server ./tests",
    "test:e2e": "bun run scripts/run-e2e.ts --reporter=list",
    "test:e2e:ui": "bun run scripts/run-e2e.ts --ui",
    "test:e2e:debug": "bun run scripts/run-e2e.ts --debug",
    "test:e2e:headed": "bun run scripts/run-e2e.ts --headed --reporter=list",
    "test:e2e:chromium": "bun run scripts/run-e2e.ts --project=chromium",
    "test:e2e:report": "playwright show-report ../../test-output/e2e/playwright",
    "test:e2e:only": "playwright test --reporter=list",
    "ladle": "ladle serve",
    "ladle:build": "ladle build"
  },
  "dependencies": {
    "better-auth": "^1.4.17",
