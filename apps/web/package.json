{
  "name": "@vamsa/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "start": "bun run server/index.ts",
    "start:prod": "NODE_ENV=production bun run server/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "LOG_LEVEL=silent bun run vitest run",
    "test:int": "bun test ./tests/integration --timeout 30000",
    "test:e2e": "bun --env-file=../../.env run scripts/run-e2e.ts --reporter=list",
    "test:e2e:report": "playwright show-report ../../test-output/e2e/playwright",
    "test:perf": "bun --env-file=../../.env run scripts/run-e2e.ts --project=performance --reporter=list",
    "test:visual": "bun --env-file=../../.env run scripts/run-e2e.ts --project=visual --reporter=list",
    "test:visual:update": "bun --env-file=../../.env run scripts/run-e2e.ts --project=visual --update-snapshots --reporter=list",
    "check:bundle": "bun scripts/check-bundle-size.ts",
    "ladle": "ladle serve",
    "ladle:build": "ladle build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
