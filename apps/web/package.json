    "node-cron": "^4.2.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.3",
    "recharts": "^3.6.0",
    "svg2pdf.js": "^2.7.0",
    "zod": "^4.3.5"
