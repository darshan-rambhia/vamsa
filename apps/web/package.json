    "@tanstack/react-start": "^1.150.0",
    "@types/d3": "^7.4.3",
    "@types/maplibre-gl": "^1.14.0",
    "@vamsa/api": "workspace:*",
    "@vamsa/lib": "workspace:*",
    "@vamsa/schemas": "workspace:*",
    "@vamsa/ui": "workspace:*",
    "archiver": "^7.0.1",
    "arctic": "^3.7.0",
    "d3": "^7.9.0",
    "hono": "^4.11.4",
    "hono-rate-limiter": "^0.5.3",
