# ============================================
# E2E TEST ENVIRONMENT
# ============================================
# Used by Playwright for local E2E testing
# These values are NOT secrets - they're test credentials

# Test database (port 5433 to avoid conflict with dev db)
DATABASE_URL="postgresql://vamsa_test:vamsa_test@localhost:5433/vamsa_test?sslmode=disable"

# Auth configuration for test environment
BETTER_AUTH_SECRET="e2e-test-secret-32-characters-long"
BETTER_AUTH_URL="http://localhost:3000"
APP_URL="http://localhost:3000"

# NOTE: E2E_TESTING is set by Playwright config, not here
# This prevents unit tests from having rate limiting disabled

# E2E test users (created by seed-e2e.ts)
E2E_ADMIN_EMAIL="admin@test.vamsa.local"
E2E_ADMIN_PASSWORD="TestAdmin123!"
E2E_MEMBER_EMAIL="member@test.vamsa.local"
E2E_MEMBER_PASSWORD="TestMember123!"
SKIP_SSL_VERIFY=true